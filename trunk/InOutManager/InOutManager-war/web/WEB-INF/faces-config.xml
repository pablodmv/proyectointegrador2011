<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <application>
      <resource-bundle>
            <base-name>ati.manager.inout.bundle.messages</base-name>
            <var>msg</var>
       </resource-bundle>
     </application>
    <navigation-rule>
        <from-view-id>/pages/index.xhtml</from-view-id>
        <navigation-case>
          <from-outcome>verMarcas</from-outcome>
          <to-view-id>/pages/admMarcas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
          <from-outcome>altaDocente</from-outcome>
          <to-view-id>/pages/altaDocente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/altaDocente.xhtml</from-view-id>
        <navigation-case>
          <from-outcome>verMarcas</from-outcome>
          <to-view-id>/pages/admMarcas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/admMarcas.xhtml</from-view-id>
        <navigation-case>
          <from-outcome>altaDocente</from-outcome>
          <to-view-id>/pages/altaDocente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
